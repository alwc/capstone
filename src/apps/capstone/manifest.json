{
  "manifest_version": 2,
  "name": "Capstone",
  "key": "",
  "version": "3.2",
  "permissions": [
    "background",
    "clipboardRead",
    "clipboardWrite",
    "webview",
    "system.network",
    "storage",
    "fileSystem",
    "unlimitedStorage"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "webview": {
    "partitions": [
      {
        "name": "sandbox",
        "accessible_resources": ["sandbox.html", "main.js", "assets/*"]
      }
    ]
  },
  "sockets": {
    "tcp": { "connect": ["*"] },
    "udp": {
      "send": "*",
      "bind": ["*"],
      "multicastMembership": ""
    },
    "tcpServer": { "listen": ["*"] },
    "udpServer": { "listen": ["*"] }
  }
}
